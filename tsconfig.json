{
  "compilerOptions": {

    "target": "es2022",
    /* Modules */
    "module": "es2022",
    "paths": {
      "wasi:http/types@0.2.0": [ "./types/interfaces/wasi-http-types.d.ts" ],
      "wasi:keyvalue/store@0.2.0-draft": [ "./types/interfaces/wasi-keyvalue-store.d.ts" ],
      "wasi:filesystem/types@0.2.1": [ "./types/interfaces/wasi-filesystem-types.d.ts" ],
